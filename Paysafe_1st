#include<bits/stdc++.h>
using namespace std;
int main()
{
	string first,second;
	cin>>first>>second;
	int j=0;
	int forward[100005]={0},backward[100005]={0};
	for(int i=1;i<=first.size();i++)
	{
		if(j<second.size() && first[i-1]==second[j])
			j++;
		forward[i]=j;
	}
	j=0;
	for(int i=first.size();i>=1;i--)
	{
		if(j<second.size() && first[i-1]==second[second.size()-j-1])
			j++;
		backward[i]=j;
	}
	int q,s;
	cin>>q>>s;
	while(q--)
	{
		int x,y;
		cin>>x>>y;
		if(x-1+first.size()-y <second.size()){cout<<"No\n";continue;}
		if(forward[x-1]+backward[y+1]>=second.size())cout<<"Yes\n";
		else cout<<"No\n";
	}
	return 0;
}






